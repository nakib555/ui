{
  "name": "agentic-ai-backend",
  "version": "1.0.0",
  "description": "Backend API server for Agentic AI Chat",
  "type": "module",
  "main": "server.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/server.cjs",
    "dev": "nodemon --watch . --ext ts --exec node --loader ts-node/esm server.ts",
    "build": "npx playwright install chromium && esbuild server.ts --bundle --platform=node --format=cjs --outfile=dist/server.cjs --packages=external",
    "build:server": "npm run build"
  },
  "dependencies": {
    "@google/genai": "1.29.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "esbuild": "^0.20.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}