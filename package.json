
{
  "name": "agentic-ai-chat",
  "version": "1.0.0",
  "description": "A responsive chat application powered by the Gemini API, featuring an agentic AI persona.",
  "keywords": [
    "react",
    "gemini",
    "ai",
    "chat",
    "agentic",
    "math"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build && npx tailwindcss -i ./src/styles/main.css -o ./dist/styles/main.css --minify",
    "build:server": "esbuild backend/server.ts --bundle --platform=node --format=cjs --outfile=dist/server.cjs --external:playwright",
    "start": "node dist/server.cjs",
    "dev": "node dev.js",
    "deploy": "npm run build:client && wrangler pages deploy dist"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "2.20.0",
    "@google/genai": "^1.34.0",
    "ai": "^5.0.118",
    "@vitejs/plugin-react": "latest",
    "autoprefixer": "latest",
    "cors": "latest",
    "cpx": "latest",
    "dotenv": "latest",
    "esbuild": "latest",
    "express": "^4.22.1",
    "framer-motion": "12.24.7",
    "html2canvas": "1.4.1",
    "jszip": "^3.10.1",
    "playwright": "latest",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "16.1.0",
    "react-virtuoso": "^4.7.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.4.5",
    "vite": "latest",
    "wrangler": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/cors": "latest",
    "@types/express": "latest",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "nodemon": "latest",
    "ts-node": "latest"
  }
}
