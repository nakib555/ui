
{
  "name": "agentic-ai-chat",
  "version": "1.0.0",
  "description": "A responsive chat application powered by the Gemini API, featuring an agentic AI persona.",
  "keywords": [
    "react",
    "gemini",
    "ai",
    "chat",
    "agentic",
    "math"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build && npx tailwindcss -i ./src/styles/main.css -o ./dist/styles/main.css --minify",
    "build:server": "esbuild backend/server.ts --bundle --platform=node --format=cjs --outfile=dist/server.cjs --external:playwright",
    "start": "node dist/server.cjs",
    "dev": "node dev.js",
    "deploy": "npm run build:client && wrangler pages deploy dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "latest",
    "@google/genai": "latest",
    "@vitejs/plugin-react": "latest",
    "ai": "latest",
    "autoprefixer": "^10.4.14",
    "cors": "latest",
    "cpx": "^latest",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "express": "^4.19.2",
    "framer-motion": "11.2.10",
    "html2canvas": "1.4.1",
    "jszip": "^3.10.1",
    "playwright": "^1.49.0",
    "postcss": "^8.4.30",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-katex": "^3.0.4",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "15.5.0",
    "react-virtuoso": "^4.6.2",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "6.0.0",
    "tailwindcss": "^3.4.8",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "wrangler": "^4.56.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "babel-plugin-react-compiler": "0.0.0-experimental-c8b3f72-20240517",
    "eslint": "^9.0.0",
    "eslint-plugin-react-compiler": "0.0.0-experimental-c8b3f72-20240517",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
